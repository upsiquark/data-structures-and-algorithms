    "#implementation of a graph\n",
    "\n",
    "class Vertex:\n",
    "    def __init__(self, value):\n",
    "        self.value = value\n",
    "        self.adjacent_vertices = []\n",
    "\n",
    "    def add_adjacent_vertex(self, vertex):\n",
    "        self.adjacent_vertices.append(vertex)"
